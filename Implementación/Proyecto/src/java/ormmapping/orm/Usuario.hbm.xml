<hibernate-mapping>
  <class lazy="false" name="ORM.Usuario" table="`Usuario`">
    <id column="`idUsuario`" name="idUsuario" type="integer" unsaved-value="0">
      <generator class="increment">
			</generator>
    </id>
    <property column="`nombreUsuario`" lazy="false" length="15" name="nombreUsuario" not-null="true" type="string" unique="true"/>
    <property column="`contraseñaUsuario`" lazy="false" length="20" name="contraseñaUsuario" not-null="true" type="string"/>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_Lista">
      <key column="`UsuarioidUsuario`" not-null="true"/>
      <one-to-many class="ORM.Lista"/>
    </set>
  </class>
</hibernate-mapping>
