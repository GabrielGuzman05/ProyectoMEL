<hibernate-mapping>
  <class lazy="false" name="ORM.EntradaLista" table="`EntradaLista`">
    <id column="`idEntrada`" name="idEntrada" type="integer" unsaved-value="0">
      <generator class="increment">
			</generator>
    </id>
    <property column="`alDia`" lazy="false" length="1" name="alDia" not-null="true" type="boolean"/>
    <property column="`ultimoCapitulo`" lazy="false" length="6" name="ultimoCapitulo" not-null="true" type="integer"/>
    <property column="`terminado`" lazy="false" length="1" name="terminado" not-null="true" type="boolean"/>
    <property column="`tipo`" lazy="false" length="1" name="tipo" not-null="true" type="integer"/>
    <many-to-one access="field" cascade="lock" class="ORM.Lista" column="`ListaidLista`" lazy="proxy" name="listaidLista" not-null="true">
		</many-to-one>
    <many-to-one access="field" cascade="lock" class="ORM.Novela" column="`NovelaidNovela`" lazy="proxy" name="novelaidNovela" not-null="false">
		</many-to-one>
    <many-to-one access="field" cascade="lock" class="ORM.Manga" column="`MangaidManga`" lazy="proxy" name="mangaidManga" not-null="false">
		</many-to-one>
    <many-to-one access="field" cascade="lock" class="ORM.Animacion" column="`AnimacionidAnimacion`" lazy="proxy" name="animacionidAnimacion" not-null="false">
		</many-to-one>
    <many-to-one access="field" cascade="lock" class="ORM.SerieGenerica" column="`SerieGenericaidSerie`" lazy="proxy" name="serieGenericaidSerie" not-null="false">
		</many-to-one>
  </class>
</hibernate-mapping>
