<hibernate-mapping>
  <class lazy="false" name="ORM.LenguajeOriginal" table="`LenguajeOriginal`">
    <id column="`idLenguaje`" name="idLenguaje" type="integer" unsaved-value="0">
      <generator class="increment">
			</generator>
    </id>
    <property column="`nombre`" lazy="false" length="50" name="nombre" not-null="true" type="string" unique="true"/>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_Novela">
      <key column="`LenguajeOriginalidLenguaje`" not-null="true"/>
      <one-to-many class="ORM.Novela"/>
    </set>
  </class>
</hibernate-mapping>
