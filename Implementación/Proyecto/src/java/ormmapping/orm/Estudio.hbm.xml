<hibernate-mapping>
  <class lazy="false" name="ORM.Estudio" table="`Estudio`">
    <id column="`idEstudio`" name="idEstudio" type="integer" unsaved-value="0">
      <generator class="native">
			</generator>
    </id>
    <property column="`nombre`" lazy="false" length="255" name="nombre" not-null="false" type="string"/>
    <set cascade="save-update,lock" lazy="true" name="ORM_AnimacionidAnimacion" table="`Estudio_Animacion`">
      <key column="`EstudioidEstudio`" not-null="true"/>
      <many-to-many class="ORM.Animacion" column="`AnimacionidAnimacion`">
			</many-to-many>
    </set>
  </class>
</hibernate-mapping>
