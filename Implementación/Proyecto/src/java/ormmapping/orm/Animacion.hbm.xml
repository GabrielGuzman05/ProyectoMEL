<hibernate-mapping>
  <class lazy="false" name="ORM.Animacion" table="`Animacion`">
    <id column="`idAnimacion`" name="idAnimacion" type="integer" unsaved-value="0">
      <generator class="native">
			</generator>
    </id>
    <property column="`nombre`" lazy="false" length="255" name="nombre" not-null="false" type="string"/>
    <property column="`nombreAlternativo`" lazy="false" length="255" name="nombreAlternativo" not-null="false" type="string"/>
    <property column="`fuente`" lazy="false" length="10" name="fuente" not-null="false" type="string"/>
    <property column="`enEmision`" lazy="false" length="1" name="enEmision" not-null="false" type="byte"/>
    <property column="`episodios`" lazy="false" length="10" name="episodios" not-null="false" type="integer"/>
    <property column="`MALCode`" lazy="false" length="255" name="MALCode" not-null="false" type="string"/>
    <property column="`EstadoUsuarioidEstadoUsuario`" lazy="false" length="10" name="estadoUsuarioidEstadoUsuario" not-null="true" type="integer"/>
    <many-to-one access="field" cascade="lock" class="ORM.Temporada" column="`TemporadaidTemporada`" lazy="proxy" name="temporadaidTemporada" not-null="true">
		</many-to-one>
    <many-to-one access="field" cascade="lock" class="ORM.TipoEmision" column="`TipoEmisionidTipoEmision`" lazy="proxy" name="tipoEmisionidTipoEmision" not-null="true">
		</many-to-one>
    <many-to-one access="field" cascade="lock" class="ORM.Rating" column="`RatingidRating`" lazy="proxy" name="ratingidRating" not-null="true">
		</many-to-one>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_EstudioidEstudio" table="`Estudio_Animacion`">
      <key column="`AnimacionidAnimacion`" not-null="true"/>
      <many-to-many class="ORM.Estudio" column="`EstudioidEstudio`">
			</many-to-many>
    </set>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_GeneroidGenero" table="`Genero_Animacion`">
      <key column="`AnimacionidAnimacion`" not-null="true"/>
      <many-to-many class="ORM.Genero" column="`GeneroidGenero`">
			</many-to-many>
    </set>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_EntradaLista">
      <key column="`AnimacionidAnimacion`" not-null="false"/>
      <one-to-many class="ORM.EntradaLista"/>
    </set>
  </class>
</hibernate-mapping>
