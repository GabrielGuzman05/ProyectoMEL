<hibernate-mapping>
  <class lazy="false" name="ORM.Genero" table="`Genero`">
    <id column="`idGenero`" name="idGenero" type="integer" unsaved-value="0">
      <generator class="native">
			</generator>
    </id>
    <property column="`nombre`" lazy="false" length="255" name="nombre" not-null="true" type="string" unique="true"/>
    <property column="`descripcion`" lazy="false" length="1000" name="descripcion" not-null="false" type="string"/>
    <set cascade="save-update,lock" lazy="true" name="ORM_NovelaidNovela" table="`Genero_Novela`">
      <key column="`GeneroidGenero`" not-null="true"/>
      <many-to-many class="ORM.Novela" column="`NovelaidNovela`">
			</many-to-many>
    </set>
    <set cascade="save-update,lock" lazy="true" name="ORM_AnimacionidAnimacion" table="`Genero_Animacion`">
      <key column="`GeneroidGenero`" not-null="true"/>
      <many-to-many class="ORM.Animacion" column="`AnimacionidAnimacion`">
			</many-to-many>
    </set>
    <set cascade="save-update,lock" lazy="true" name="ORM_MangaidManga" table="`Genero_Manga`">
      <key column="`GeneroidGenero`" not-null="true"/>
      <many-to-many class="ORM.Manga" column="`MangaidManga`">
			</many-to-many>
    </set>
    <set cascade="save-update,lock" lazy="true" name="ORM_SerieGenericaidSerie" table="`Genero_SerieGenerica`">
      <key column="`GeneroidGenero`" not-null="true"/>
      <many-to-many class="ORM.SerieGenerica" column="`SerieGenericaidSerie`">
			</many-to-many>
    </set>
  </class>
</hibernate-mapping>
