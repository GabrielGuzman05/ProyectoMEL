<hibernate-mapping>
  <class lazy="false" name="ORM.Tag" table="`Tag`">
    <id column="`idTag`" name="idTag" type="integer" unsaved-value="0">
      <generator class="native">
			</generator>
    </id>
    <property column="`nombre`" lazy="false" length="50" name="nombre" not-null="true" type="string" unique="true"/>
    <property column="`descripcion`" lazy="false" length="1000" name="descripcion" not-null="false" type="string"/>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_NovelaidNovela" table="`Novela_Tag`">
      <key column="`TagidTag`" not-null="true"/>
      <many-to-many class="ORM.Novela" column="`NovelaidNovela`">
			</many-to-many>
    </set>
    <set cascade="save-update,lock" lazy="true" name="ORM_MangaidManga" table="`Tag_Manga`">
      <key column="`TagidTag`" not-null="true"/>
      <many-to-many class="ORM.Manga" column="`MangaidManga`">
			</many-to-many>
    </set>
  </class>
</hibernate-mapping>
