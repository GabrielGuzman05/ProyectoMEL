<hibernate-mapping>
  <class lazy="false" name="ORM.Temporada" table="`Temporada`">
    <id column="`idTemporada`" name="idTemporada" type="integer" unsaved-value="0">
      <generator class="native">
			</generator>
    </id>
    <property column="`estacion`" lazy="false" length="255" name="estacion" not-null="false" type="string"/>
    <property column="`año`" lazy="false" length="4" name="año" not-null="false" type="integer"/>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_Animacion">
      <key column="`TemporadaidTemporada`" not-null="true"/>
      <one-to-many class="ORM.Animacion"/>
    </set>
  </class>
</hibernate-mapping>
