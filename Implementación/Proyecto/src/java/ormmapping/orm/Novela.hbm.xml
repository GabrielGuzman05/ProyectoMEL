<hibernate-mapping>
  <class lazy="false" name="ORM.Novela" table="`Novela`">
    <id column="`idNovela`" name="idNovela" type="integer" unsaved-value="0">
      <generator class="increment">
			</generator>
    </id>
    <property column="`nombre`" lazy="false" length="255" name="nombre" not-null="false" type="string"/>
    <property column="`nombreAlternativo`" lazy="false" length="500" name="nombreAlternativo" not-null="false" type="string"/>
    <property column="`año`" lazy="false" length="4" name="año" not-null="false" type="string"/>
    <property column="`licenciado`" lazy="false" length="1" name="licenciado" not-null="false" type="byte"/>
    <property column="`completamenteTraducido`" lazy="false" length="1" name="completamenteTraducido" not-null="false" type="byte"/>
    <property column="`novelUpdateUrl`" lazy="false" length="255" name="novelUpdateUrl" not-null="false" type="string"/>
    <property column="`estrellas`" lazy="false" length="1" name="estrellas" not-null="false" type="integer"/>
    <many-to-one access="field" cascade="lock" class="ORM.PublicadorOriginal" column="`PublicadorOriginalidPublicadorOriginal`" lazy="proxy" name="publicadorOriginalidPublicadorOriginal" not-null="true">
		</many-to-one>
    <many-to-one access="field" cascade="lock" class="ORM.Autor" column="`AutoridAutor`" lazy="proxy" name="autoridAutor" not-null="true">
		</many-to-one>
    <many-to-one access="field" cascade="lock" class="ORM.PublicadorIngles" column="`PublicadorInglesidPublicadorIngles`" lazy="proxy" name="publicadorInglesidPublicadorIngles" not-null="true">
		</many-to-one>
    <many-to-one access="field" cascade="lock" class="ORM.Tipo" column="`TipoidTipo`" lazy="proxy" name="tipoidTipo" not-null="true">
		</many-to-one>
    <property column="`EstadoUsuarioidEstadoUsuario`" lazy="false" length="10" name="estadoUsuarioidEstadoUsuario" not-null="true" type="integer"/>
    <many-to-one access="field" cascade="lock" class="ORM.LenguajeOriginal" column="`LenguajeOriginalidLenguaje`" lazy="proxy" name="lenguajeOriginalidLenguaje" not-null="true">
		</many-to-one>
    <many-to-one access="field" cascade="lock" class="ORM.Artista" column="`ArtistaidArtista`" lazy="proxy" name="artistaidArtista" not-null="true">
		</many-to-one>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_GeneroidGenero" table="`Genero_Novela`">
      <key column="`NovelaidNovela`" not-null="true"/>
      <many-to-many class="ORM.Genero" column="`GeneroidGenero`">
			</many-to-many>
    </set>
    <set cascade="save-update,lock" lazy="true" name="ORM_TagidTag" table="`Novela_Tag`">
      <key column="`NovelaidNovela`" not-null="true"/>
      <many-to-many class="ORM.Tag" column="`TagidTag`">
			</many-to-many>
    </set>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_EntradaLista">
      <key column="`NovelaidNovela`" not-null="false"/>
      <one-to-many class="ORM.EntradaLista"/>
    </set>
  </class>
</hibernate-mapping>
