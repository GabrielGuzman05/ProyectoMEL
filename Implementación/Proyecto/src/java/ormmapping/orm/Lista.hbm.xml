<hibernate-mapping>
  <class lazy="false" name="ORM.Lista" table="`Lista`">
    <id column="`idLista`" name="idLista" type="integer" unsaved-value="0">
      <generator class="increment">
			</generator>
    </id>
    <property column="`nombreLista`" lazy="false" length="15" name="nombreLista" not-null="true" type="string"/>
    <many-to-one access="field" cascade="lock" class="ORM.Usuario" column="`UsuarioidUsuario`" lazy="proxy" name="usuarioidUsuario" not-null="true">
		</many-to-one>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_EntradaLista">
      <key column="`ListaidLista`" not-null="true"/>
      <one-to-many class="ORM.EntradaLista"/>
    </set>
  </class>
</hibernate-mapping>
