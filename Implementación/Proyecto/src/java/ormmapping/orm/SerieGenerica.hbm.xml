<hibernate-mapping>
  <class lazy="false" name="ORM.SerieGenerica" table="`SerieGenerica`">
    <id column="`idSerie`" name="idSerie" type="integer" unsaved-value="0">
      <generator class="native">
			</generator>
    </id>
    <property column="`nombre`" lazy="false" length="255" name="nombre" not-null="false" type="string"/>
    <property column="`nombreAlternativo`" lazy="false" length="255" name="nombreAlternativo" not-null="false" type="string"/>
    <property column="`cadena`" lazy="false" length="255" name="cadena" not-null="false" type="string"/>
    <property column="`episodios`" lazy="false" length="10" name="episodios" not-null="false" type="integer"/>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_GeneroidGenero" table="`Genero_SerieGenerica`">
      <key column="`SerieGenericaidSerie`" not-null="true"/>
      <many-to-many class="ORM.Genero" column="`GeneroidGenero`">
			</many-to-many>
    </set>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_EntradaLista">
      <key column="`SerieGenericaidSerie`" not-null="false"/>
      <one-to-many class="ORM.EntradaLista"/>
    </set>
  </class>
</hibernate-mapping>
