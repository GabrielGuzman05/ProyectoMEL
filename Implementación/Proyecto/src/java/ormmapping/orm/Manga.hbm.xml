<hibernate-mapping>
  <class lazy="false" name="ORM.Manga" table="`Manga`">
    <id column="`idManga`" name="idManga" type="integer" unsaved-value="0">
      <generator class="native">
			</generator>
    </id>
    <property column="`EstadoUsuarioidEstadoUsuario`" lazy="false" length="10" name="estadoUsuarioidEstadoUsuario" not-null="true" type="integer"/>
    <property column="`nombreManga`" lazy="false" length="255" name="nombreManga" not-null="false" type="string"/>
    <property column="`nombreAlternativo`" lazy="false" length="255" name="nombreAlternativo" not-null="false" type="string"/>
    <property column="`completamenteScanlated`" lazy="false" length="1" name="completamenteScanlated" not-null="false" type="byte"/>
    <property column="`tieneAnimacion`" lazy="false" length="1" name="tieneAnimacion" not-null="false" type="byte"/>
    <property column="`año`" lazy="false" length="4" name="año" not-null="false" type="string"/>
    <property column="`licenciado`" lazy="false" length="1" name="licenciado" not-null="true" type="byte"/>
    <property column="`bakaUpdatesCode`" lazy="false" length="255" name="bakaUpdatesCode" not-null="false" type="string"/>
    <property column="`estadoEnPaisDeOrigen`" lazy="false" length="255" name="estadoEnPaisDeOrigen" not-null="false" type="string"/>
    <property column="`animeStartEnd`" lazy="false" length="255" name="animeStartEnd" not-null="false" type="string"/>
    <many-to-one access="field" cascade="lock" class="ORM.TipoManga" column="`TipoMangaidTipoManga`" lazy="proxy" name="tipoMangaidTipoManga" not-null="false">
		</many-to-one>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_GeneroidGenero" table="`Genero_Manga`">
      <key column="`MangaidManga`" not-null="true"/>
      <many-to-many class="ORM.Genero" column="`GeneroidGenero`">
			</many-to-many>
    </set>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_AutoridAutor" table="`Autor_Manga`">
      <key column="`MangaidManga`" not-null="true"/>
      <many-to-many class="ORM.Autor" column="`AutoridAutor`">
			</many-to-many>
    </set>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_TagidTag" table="`Tag_Manga`">
      <key column="`MangaidManga`" not-null="true"/>
      <many-to-many class="ORM.Tag" column="`TagidTag`">
			</many-to-many>
    </set>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_AristaidArtista" table="`Arista_Manga`">
      <key column="`MangaidManga`" not-null="true"/>
      <many-to-many class="ORM.Artista" column="`AristaidArtista`">
			</many-to-many>
    </set>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_EntradaLista">
      <key column="`MangaidManga`" not-null="false"/>
      <one-to-many class="ORM.EntradaLista"/>
    </set>
  </class>
</hibernate-mapping>
