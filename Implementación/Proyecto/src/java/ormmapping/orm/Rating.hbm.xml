<hibernate-mapping>
  <class lazy="false" name="ORM.Rating" table="`Rating`">
    <id column="`idRating`" name="idRating" type="integer" unsaved-value="0">
      <generator class="native">
			</generator>
    </id>
    <property column="`nombre`" lazy="false" length="255" name="nombre" not-null="false" type="string"/>
    <set cascade="save-update,lock" inverse="true" lazy="true" name="ORM_Animacion">
      <key column="`RatingidRating`" not-null="true"/>
      <one-to-many class="ORM.Animacion"/>
    </set>
  </class>
</hibernate-mapping>
